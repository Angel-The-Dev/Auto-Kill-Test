local TimeSpentLabel = MainTab:AddLabel("Time Spent in Server: 0d 0h 0m 0s")
local CurrentTime = os.time()
local function UpdateFunction(TimeParameter)
    local DaysPassed = math.floor(TimeParameter / 86400)
    local RemainingSeconds = TimeParameter % 86400
    local HoursPassed = math.floor(RemainingSeconds / 3600)
    local RemainingMinutes = RemainingSeconds % 3600
    local MinutesPassed = math.floor(RemainingMinutes / 60)
    local SecondsPassed = RemainingMinutes % 60
    return string.format("%dd %dh %dm %ds", DaysPassed, HoursPassed, MinutesPassed, SecondsPassed)
end
