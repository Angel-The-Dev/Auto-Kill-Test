    local v218 = 0
    for v219 = - v214, v214 do
        local v220 = v219
        for v221 = - v217, v217 do
            local v222 = Instance.new("Part")
            v222.Parent = game.Workspace
            v222.Anchored = true
            v222.Position = Vector3.new(v220 * (v213.X + v218), - 9, v221 * (v213.Z + v218))
            v222.Size = v213
            v222.Transparency = 1
        end
    end


